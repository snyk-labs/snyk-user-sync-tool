{
  "name": "snyk-user-sync-tool",
  "version": "2.2.3",
  "description": "sync snyk org users from an external source",
  "main": "dist/index.js",
  "scripts": {
    "format:check": "prettier --check '{''{src,test}/!(fixtures)/**/*,*}.{js,ts,json,yml}'",
    "format": "prettier --write '{''{src,test}/!(fixtures)/**/*,*}.{js,ts,json,yml}'",
    "lint": "npm run format:check && npm run lint:eslint",
    "lint:eslint": "eslint --cache '{lib,test}/**/*.ts'",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest --verbose",
    "test:coverage": "npm run test:unit -- --coverage",
    "test:watch": "tsc-watch --onSuccess 'npm run test:unit'",
    "build": "tsc",
    "build-watch": "tsc -w",
    "prepare": "npm run build",
    "snyk-test": "snyk test",
    "nexe-compile": "nexe dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk-tech-services/snyk-user-sync-tool"
  },
  "author": "Snyk Tech Services",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=12"
  },
  "files": [
    "bin",
    "dist",
    "conf"
  ],
  "homepage": "https://github.com/snyk-tech-services/snyk-user-sync-tool#readme",
  "dependencies": {
    "debug": "^4.1.1",
    "ora": "^5.4.1",
    "p-map": "4.0.0",
    "snyk-request-manager": "^1.4.1",
    "tslib": "^1.10.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "@types/node": "^12.12.26",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "jest": "^26.6.3",
    "prettier": "^1.19.1",
    "ts-jest": "^26.5.5",
    "nexe": "^4.0.0-beta.17",
    "tsc-watch": "^4.1.0",
    "typescript": "^4.2.4"
  },
  "pkg": {
    "scripts": [
      "dist/**/*.js"
    ]
  },
  "bin": {
    "snyk-user-sync-tool": "./dist/index.js"
  },
  "release": {
    "branches": [
      "master"
    ]
  }
}
